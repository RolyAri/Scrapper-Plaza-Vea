{
    "name": "plaza-vea-scrapper-reto",
    "description": "Extrae la informaci√≥n de los productos de plaza vea",
    "manifest_version": 3,
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "tabs", "storage", "downloads"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [
          "https://www.plazavea.com.pe/*"
        ],
        "js": ["./contentscript.js"]
      }
    ],
    "action": {
      "default_popup": "./popup/index.html"
    }
  }
  